generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
}

model User {
  id                String   @id @default(uuid())
  firstName         String
  lastName          String
  email             String   @unique
  username          String   @unique
  password          String
  avatar            String?  @default("")
  dateJoined        DateTime @default(now())
  lastProfileUpdate DateTime @updatedAt
  isDeleted         Boolean  @default(false)

}

